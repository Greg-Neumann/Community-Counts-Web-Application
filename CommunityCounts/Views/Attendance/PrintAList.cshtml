@model  IEnumerable<CommunityCounts.Models.Master.C1client>
@{
    ViewBag.Title = "Print Attendance";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Attendance list</h2>
<h3>@ViewBag.ActivityName - @ViewBag.StartDate at @ViewBag.StartTime</h3>
<table>
    <tr>
        <th>

        </th>
        <th>
            <h4>First &nbsp;</h4>
        </th>
        <th>
            <h4>Last </h4>
        </th>
        <th>
            &nbsp&nbsp&nbsp
        </th>
        <th>
            <h4>@ViewBag.ListHeader</h4>
        </th>
    </tr>
    @{int i = 1;}
    @foreach (var item in Model)
    {
        <tr>
            <th>
                <h5> @i.ToString("D3")&nbsp;</h5>
            </th>
            <th>
                <h5>@Html.DisplayFor(modelItem => item.FirstName)&nbsp;</h5>
            </th>
            <th>
                <h5>@Html.DisplayFor(modelItem => item.LastName)</h5>
            </th>
            <th>
                &nbsp&nbsp&nbsp
            </th>
            <th>
                <h5>@ViewBag.ListFormat</h5>
            </th>
        </tr>
        i++;
    }
</table>