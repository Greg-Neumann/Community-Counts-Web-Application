@model CommunityCounts.Models.Master.regyear
@{
    ViewBag.Title = "Change Registraction Year";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Change registration year</h2>

@using (Html.BeginForm())
{
@Html.AntiForgeryToken()
<div class="form-horizontal">
    <hr />
    @Html.ActionLink("Back to List", "Index", null, htmlAttributes: new { @class = "btn btn-primary btn-xs" })
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        @Html.LabelFor(model => model.idRegYear, "Registration Year", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-2">
            @Html.DropDownList("idRegYear", null, htmlAttributes: new { @class = "form-control" })
        </div>
    </div>
</div>
<div class="form-group">
    <div class="col-md-offset-1 col-md-10">
        <input type="submit" value="Change" class="btn btn-warning" />
    </div>
</div>
    }
